{
  "name": "getpetvet",
  "version": "1.0.0",
  "description": "Virtual Vet Visit App",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "start:server": "nodemon server/server.js",
    "start:client": "cd client && npm run start",
    "dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "install": "cd client && npm i",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.7",
    "nodemon": "^2.0.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaslenning/GetPetVet.git"
  },
  "author": "Seth Martineau, Syndey Goodwill, Nikolas Lenning, Zacarie LaFleur",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nikolaslenning/GetPetVet/issues"
  },
  "homepage": "https://github.com/nikolaslenning/GetPetVet#readme"
}
